#define Handlers
handlers=java.util.logging.FileHandler
# Setup the File Handler
# Do not use \ -- this is parsed as a Java string!
java.util.logging.FileHandler.pattern=./tests/testlogs/netarkivtest.log
java.util.logging.FileHandler.limit=1500000
java.util.logging.FileHandler.count=1
java.util.logging.FileHandler.append=false
java.util.logging.FileHandler.formatter=java.util.logging.SimpleFormatter

# Logging levels
.level=WARNING
dk.netarkivet.level=ALL